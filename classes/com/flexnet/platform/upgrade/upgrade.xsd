<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSPY v5 rel. 4 U (http://www.xmlspy.com) by Kevin Thompson --><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"	elementFormDefault="qualified" attributeFormDefault="unqualified">	<xs:simpleType name="variable_name">		<xs:annotation>			<xs:documentation>Type restricting the names of the variables to				start with : (colon)</xs:documentation>		</xs:annotation>		<xs:restriction base="xs:string">			<xs:pattern value=":\S+" />		</xs:restriction>	</xs:simpleType>	<xs:element name="boolean" type="xs:string">		<xs:annotation>			<xs:documentation>Boolean value (represented as string)			</xs:documentation>		</xs:annotation>	</xs:element>	<xs:element name="timestamp" type="xs:string">		<xs:annotation>			<xs:documentation>Timestamp in ISO format YYYY-MM-DD HH:mm:ss				(represented as string) or a word now which represent current moment			</xs:documentation>		</xs:annotation>	</xs:element>	<xs:element name="numeric" type="xs:string">		<xs:annotation>			<xs:documentation>Numeric value (represented as string)			</xs:documentation>		</xs:annotation>	</xs:element>	<xs:element name="string" type="xs:string">		<xs:annotation>			<xs:documentation>String value</xs:documentation>		</xs:annotation>	</xs:element>	<xs:element name="column">		<xs:annotation>			<xs:documentation>Definition of the column</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="table" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Name of the column</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="name" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Name of the column</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="type" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Type of the column. Database independent					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="size" type="xs:integer" use="optional">				<xs:annotation>					<xs:documentation>Size of the column if the column is string					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="nullable" type="xs:boolean" use="optional">				<xs:annotation>					<xs:documentation>Indicates whether column is nullable					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="default" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Name of the variable holding the default value					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="autoincrement" type="xs:boolean"				use="optional">				<xs:annotation>					<xs:documentation>Depreecated</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="identity" type="xs:boolean" use="optional">				<xs:annotation>					<xs:documentation>Indicates whether column value is an identity						column (for ID)</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="unique" type="xs:boolean" use="optional">				<xs:annotation>					<xs:documentation>Indicates whether a unique constraint should be						put on column</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="pk">		<xs:annotation>			<xs:documentation>Primary key</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="table" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Table name for which primary key is created					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="name" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Name of the primary key</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="columns" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Comma-separated list of columns in primary key					</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="fk">		<xs:annotation>			<xs:documentation>Foreign key</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="name" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Name of the foreign key</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="table" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Table name for which foreign key is created					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="columns" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Comma-separated list of columns in the foreign						key</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="ref_table" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Name of the referenced table</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="ref_columns" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Comma-separated list of referenced columns					</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="unique">		<xs:annotation>			<xs:documentation>Unique</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="table" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Table name for which unique contraint is created					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="columns" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Comma-separated list of columns in the contraint					</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="get_value">		<xs:annotation>			<xs:documentation>Simple directive to execute some SQL and store the				results in a variable</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="sql" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>SQL that has to be executed</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="var" type="variable_name" use="required">				<xs:annotation>					<xs:documentation>Variable name. Should start with :					</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="free_value">		<xs:annotation>			<xs:documentation>Simple directive to free a variable			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="var" type="variable_name" use="required">				<xs:annotation>					<xs:documentation>Name of the variable to be freed					</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="set_value">		<xs:annotation>			<xs:documentation>Simple directive to set a variable to some				hardcoded value</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:choice>				<xs:element ref="boolean" />				<xs:element ref="timestamp" />				<xs:element ref="numeric" />				<xs:element ref="string" />			</xs:choice>			<xs:attribute name="var" type="variable_name" use="required">				<xs:annotation>					<xs:documentation>Variable name. Should start with :					</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="insert_data">		<xs:annotation>			<xs:documentation>Simple directive to insert rows into table			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="table" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Name of the table where data is being inserted					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="values" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Comma-separated list of values</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="update_data">		<xs:annotation>			<xs:documentation>Simple directive to update rows in table			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="table" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Name of the table containing data being modified					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="set" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Comma-separated list of the columns being updated					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="where" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Comma-separated list of criteria that specifies						which rows must be updated.</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="delete_data">		<xs:annotation>			<xs:documentation>Simple directive to delete rows in table			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="table" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Name of the table containing datab being deleted					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="where" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Comma-separated list of criteria that specifies						which rows must be deleted</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="add_pk">		<xs:annotation>			<xs:documentation>Simple directive for adding primary keys			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence maxOccurs="unbounded">				<xs:element ref="pk" />			</xs:sequence>			<xs:attribute name="table" type="xs:string" use="optional" />		</xs:complexType>	</xs:element>	<xs:element name="drop_pk">		<xs:annotation>			<xs:documentation>Simple directive for dropping a primary key			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence maxOccurs="unbounded">				<xs:element ref="pk" />			</xs:sequence>			<xs:attribute name="table" type="xs:string" use="optional" />		</xs:complexType>	</xs:element>	<xs:element name="add_fk">		<xs:annotation>			<xs:documentation>Simple directive for adding foreign keys			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence maxOccurs="unbounded">				<xs:element ref="fk" />			</xs:sequence>			<xs:attribute name="table" type="xs:string" use="optional" />		</xs:complexType>	</xs:element>	<xs:element name="drop_fk">		<xs:annotation>			<xs:documentation>Simple directive for dropping a foreign key			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence maxOccurs="unbounded">				<xs:element ref="fk" />			</xs:sequence>			<xs:attribute name="table" type="xs:string" use="optional" />		</xs:complexType>	</xs:element>	<xs:element name="add_unique">		<xs:annotation>			<xs:documentation>Simple directive for adding an unique contraint			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence maxOccurs="unbounded">				<xs:element ref="unique" />			</xs:sequence>			<xs:attribute name="table" type="xs:string" use="optional" />		</xs:complexType>	</xs:element>	<xs:element name="drop_unique">		<xs:annotation>			<xs:documentation>Simple directive for dropping unique contraint			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence maxOccurs="unbounded">				<xs:element ref="unique" />			</xs:sequence>			<xs:attribute name="table" type="xs:string" use="optional" />		</xs:complexType>	</xs:element>	<xs:element name="add_column">		<xs:annotation>			<xs:documentation>Simple directive for adding a column			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence maxOccurs="unbounded">				<xs:element ref="column" />			</xs:sequence>			<xs:attribute name="table" type="xs:string" use="optional" />		</xs:complexType>	</xs:element>	<xs:element name="modify_column">		<xs:annotation>			<xs:documentation>Simple directive for modifying a column			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence maxOccurs="unbounded">				<xs:element ref="column" />			</xs:sequence>			<xs:attribute name="table" type="xs:string" use="optional" />		</xs:complexType>	</xs:element>	<xs:element name="drop_column">		<xs:annotation>			<xs:documentation>Simple directive for dropping a table			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence maxOccurs="unbounded">				<xs:element ref="column" />			</xs:sequence>			<xs:attribute name="table" type="xs:string" use="optional" />		</xs:complexType>	</xs:element>	<xs:element name="add_table">		<xs:annotation>			<xs:documentation>Simple directive for adding a table			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:sequence maxOccurs="unbounded">					<xs:element ref="column" />				</xs:sequence>				<xs:sequence minOccurs="0" maxOccurs="1">					<xs:element ref="pk" />				</xs:sequence>				<xs:sequence minOccurs="0" maxOccurs="unbounded">					<xs:element ref="fk" />				</xs:sequence>				<xs:sequence minOccurs="0" maxOccurs="unbounded">					<xs:element ref="unique" />				</xs:sequence>			</xs:sequence>			<xs:attribute name="name" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Name of the table being added</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="type" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Deprecated</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="alter_table">		<xs:annotation>			<xs:documentation>Simple directive for altering table. Only used with				MySQL.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="name" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Name of the table being altered					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="type" type="xs:string" use="optional">				<xs:annotation>					<xs:documentation>Deprecated</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="engine" type="xs:string" use="optional" />		</xs:complexType>	</xs:element>	<xs:element name="rename_table">		<xs:annotation>			<xs:documentation>Simple directive for renaming a table			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="old_name" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Old table name</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="new_name" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>New table name</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="drop_table">		<xs:annotation>			<xs:documentation>Simple directive for dropping a table			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="name" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Name of table being dropped</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="migrate_config_values">		<xs:annotation>			<xs:documentation>To migrate App Config values to new tables			</xs:documentation>		</xs:annotation>	</xs:element>	<xs:element name="upgrade_config_values">		<xs:annotation>			<xs:documentation>To upgrade App Config values to 12.11.0.2			</xs:documentation>		</xs:annotation>	</xs:element>	<xs:element name="migrate_fne_data">		<xs:annotation>			<xs:documentation>To migrate FNE data to LFS			</xs:documentation>		</xs:annotation>	</xs:element>	<xs:element name="identity_insert">		<xs:annotation>			<xs:documentation>Enable/disable inserting identity column values			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="table" type="xs:string" use="required" />			<xs:attribute name="enable" type="xs:boolean" use="required" />		</xs:complexType>	</xs:element>	<xs:element name="if_no_record">		<xs:annotation>			<xs:documentation>Check if there is no matching record available			</xs:documentation>		</xs:annotation>		<xs:complexType>		<xs:sequence minOccurs="0" maxOccurs="unbounded">			<xs:element ref="insert_data" minOccurs="0" maxOccurs="unbounded"/>		</xs:sequence>					<xs:attribute name="var" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>If no record available					</xs:documentation>				</xs:annotation>		</xs:attribute>				</xs:complexType>	</xs:element>	<xs:element name="for">		<xs:annotation>			<xs:documentation>The element representing for loop			</xs:documentation>		</xs:annotation>		<xs:complexType mixed="false">			<xs:sequence minOccurs="0" maxOccurs="unbounded">				<xs:element ref="get_value" minOccurs="0" />				<xs:element ref="free_value" minOccurs="0" />				<xs:element ref="set_value" minOccurs="0" />				<xs:element ref="insert_data" minOccurs="0" />				<xs:element ref="update_data" minOccurs="0" />				<xs:element ref="delete_data" minOccurs="0" />				<xs:element ref="add_pk" minOccurs="0" />				<xs:element ref="drop_pk" minOccurs="0" />				<xs:element ref="add_fk" minOccurs="0" />				<xs:element ref="drop_fk" minOccurs="0" />				<xs:element ref="add_unique" minOccurs="0" />				<xs:element ref="drop_unique" minOccurs="0" />				<xs:element ref="add_column" minOccurs="0" />				<xs:element ref="modify_column" minOccurs="0" />				<xs:element ref="drop_column" minOccurs="0" />				<xs:element ref="add_table" minOccurs="0" />				<xs:element ref="alter_table" minOccurs="0" />				<xs:element ref="rename_table" minOccurs="0" />				<xs:element ref="drop_table" minOccurs="0" />				<xs:element ref="identity_insert" minOccurs="0" />				<xs:element ref="migrate_config_values" minOccurs="0" />				<xs:element ref="migrate_fne_data" minOccurs="0" />			</xs:sequence>			<xs:attribute name="start_condition" type="xs:string"				use="required">				<xs:annotation>					<xs:documentation>Start condition for the for loop					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="end_condition" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Exit condition for the for loop					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="increment" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Increment operation performed on each iteration					</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="upgrade">		<xs:annotation>			<xs:documentation>The toplevel upgrade element which contains the				description of upgrade</xs:documentation>		</xs:annotation>		<xs:complexType mixed="false">			<xs:sequence minOccurs="0" maxOccurs="unbounded">				<xs:element ref="get_value" minOccurs="0" />				<xs:element ref="free_value" minOccurs="0" />				<xs:element ref="set_value" minOccurs="0" />				<xs:element ref="insert_data" minOccurs="0" />				<xs:element ref="update_data" minOccurs="0" />				<xs:element ref="delete_data" minOccurs="0" />				<xs:element ref="add_pk" minOccurs="0" />				<xs:element ref="drop_pk" minOccurs="0" />				<xs:element ref="add_fk" minOccurs="0" />				<xs:element ref="drop_fk" minOccurs="0" />				<xs:element ref="add_unique" minOccurs="0" />				<xs:element ref="drop_unique" minOccurs="0" />				<xs:element ref="add_column" minOccurs="0" />				<xs:element ref="modify_column" minOccurs="0" />				<xs:element ref="drop_column" minOccurs="0" />				<xs:element ref="add_table" minOccurs="0" />				<xs:element ref="alter_table" minOccurs="0" />				<xs:element ref="rename_table" minOccurs="0" />				<xs:element ref="drop_table" minOccurs="0" />				<xs:element ref="identity_insert" minOccurs="0" />				<xs:element ref="if_no_record" minOccurs="0" />				<xs:element ref="for" minOccurs="0" />			</xs:sequence>			<xs:attribute name="module" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Name of the module being upgraded					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="from_version" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Version of the schema before upgrade					</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="to_version" type="xs:string" use="required">				<xs:annotation>					<xs:documentation>Version of the schema after upgrade					</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element></xs:schema>