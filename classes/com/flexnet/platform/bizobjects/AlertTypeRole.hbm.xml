<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"><hibernate-mapping>    <class lazy="true" name="com.flexnet.platform.bizobjects.AlertTypeRole" table="PLT_ALERTTYPE_ROLE">        <meta attribute="generated-class">com.flexnet.platform.entities.AlertTypeRoleData</meta>        <meta attribute="extends" inherit="false">com.flexnet.platform.entities.Entity</meta>        <meta attribute="implements" inherit="false">com.flexnet.operations.bizobjects.ITenantIdUpdateAware</meta>        <composite-id>            <key-many-to-one name="alertType" class="com.flexnet.platform.entities.AlertType" column="ALERTTYPE_ID" >                <meta attribute="use-in-equals">true</meta>            </key-many-to-one>            <key-many-to-one name="role" class="com.flexnet.platform.bizobjects.Role" column="ROLES_ID">                <meta attribute="use-in-equals">true</meta>            </key-many-to-one>        </composite-id>        <property name="tenantId_" type="string" not-null="false" length="255">            <meta attribute="scope-set">protected</meta>            <meta attribute="scope-get">protected</meta>            <meta attribute="use-in-equals">false</meta>            <column name="TENANT_ID" not-null="false" />        </property>    </class></hibernate-mapping>