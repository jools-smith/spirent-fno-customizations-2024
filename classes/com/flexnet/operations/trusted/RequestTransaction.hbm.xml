<?xml version="1.0"?>
  <!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.flexnet.operations.trusted.RequestTransaction"
		table="OPS_REQUEST_TRANSACTION">
		<meta attribute="class-description">
			This class represents the entity that keeps track
			of request/reponse
			transactions.
			The id represents the connection
			handle and is auto generated by the
			database.
			This entity is deleted on
			a disconnect or after a configured amount of
			time.
		</meta>
		<meta attribute="generated-class" inherit="false">com.flexnet.operations.entities.RequestTransactionData</meta>
		<meta attribute="extends" inherit="false">com.flexnet.platform.entities.Entity</meta>
		<meta attribute="implements">com.flexnet.platform.bizobjects.IMarkTimestampable</meta>
		<meta attribute="scope-field">private</meta>

		<id name="id" type="java.lang.Long" column="ID" unsaved-value="null">
			<meta attribute="scope-set">protected</meta>
			<generator class="native" />
		</id>
		<version column="HIB_VERSION" name="HibVersion">
			<meta attribute="scope-get">protected</meta>
			<meta attribute="scope-set">protected</meta>
		</version>
			<property name="tenantId_" type="string" not-null="true" length="255"> 
			<meta attribute="scope-get">protected</meta> 
			<meta attribute="scope-set">protected</meta> 
			<column name="TENANT_ID" not-null="true"/> 
			</property>
		<property column="LAST_MODIFIED"
			type="com.flexnet.platform.services.persistence.GMTTimestampType"
			name="lastModified" not-null="true">
			<meta attribute="property-type">java.util.Date</meta>
		</property>
		<property name="createdOn" column="CREATED_ON"
			type="com.flexnet.platform.services.persistence.GMTTimestampType"
			not-null="true">
			<meta attribute="scope-set">protected</meta>
			<meta attribute="property-type">java.util.Date</meta>
		</property>
		<property name="sequenceNumber" type="long">
			<column name="REQ_SEQUENCE"></column>
		</property>
		<property name="machineBinding" type="string" length="64">
			<column name="MC_BINDING"></column>
		</property>
		<property name="request"
			type="com.flexnet.platform.services.persistence.StringClobType"
			length="8000">
			<meta attribute="property-type">java.lang.String</meta>
			<column name="REQ_REQUEST"></column>
		</property>
		<property name="response"
			type="com.flexnet.platform.services.persistence.StringClobType"
			length="8000">
			<meta attribute="property-type">java.lang.String</meta>
			<column name="REQ_RESPONSE"></column>
		</property>
		<property name="status" type="string" length="64">
			<column name="REQ_STATUS" index="OPS_IDX_REQT_RS_RP"></column>
		</property>
		<property name="processed" type="boolean" column="REQ_PROCESSED" />
		<property name="processedBy" type="string" length="64">
			<column name="REQ_PROCESSED_BY" index="OPS_IDX_REQT_RS_RP"></column>
		</property>
		<property name="fnpVersion" type="string" length="64">
			<column name="FNP_VERSION"></column>
		</property>
		<property name="fnpHandle" type="java.lang.Long" >
			<column name="FNP_CONNECTION_HANDLE"></column>
		</property>
		<set name="requestItems" inverse="true" cascade="all" lazy="true">
			<key column="REQ_TRANSACTION_ID" />
			<one-to-many class="com.flexnet.operations.trusted.RequestItem" />
		</set>

	</class>
</hibernate-mapping>


