<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"><hibernate-mapping>    <class lazy="true" name="com.flexnet.products.bizobjects.OrderableHostType" table="PROD_ORDERABLE_HOST_TYPE">        <meta attribute="generated-class">com.flexnet.platform.entities.OrderableHostTypeData</meta>        <meta attribute="extends" inherit="false">com.flexnet.platform.entities.Entity</meta>        <meta attribute="implements" inherit="false">com.flexnet.operations.bizobjects.ITenantIdUpdateAware</meta>        <id name="id" type="java.lang.Long" column="ID">            <generator class="native"/>        </id>        <version column="HIB_VERSION" name="HibVersion"/>        <many-to-one name="hostType" class="com.flexnet.products.bizobjects.HostTypeDO"                     column="HOST_TYPE_ID" unique-key="oht_unique_key_ord_hosttype">            <meta attribute="use-in-equals">true</meta>        </many-to-one>        <many-to-one name="orderable" class="com.flexnet.products.bizobjects.Orderable"                     column="LPORDERABLE_ID" unique-key="oht_unique_key_ord_hosttype">            <meta attribute="use-in-equals">true</meta>            <meta attribute="insert">false</meta>            <meta attribute="update">false</meta>        </many-to-one>        <property name="tenantId_" type="string" not-null="false" length="255">            <meta attribute="scope-set">protected</meta>            <meta attribute="scope-get">protected</meta>            <meta attribute="use-in-equals">false</meta>            <column name="TENANT_ID" not-null="false"/>        </property>    </class></hibernate-mapping>